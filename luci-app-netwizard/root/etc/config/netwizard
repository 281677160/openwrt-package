	option lan_ipaddr '192.168.10.1'
	option lan_gateway '192.168.10'
        option redirectdns '1'
        option lan_dhcp '1'
        option synflood '1'
        option lan_snat '1'
        option dnsset '1'
        option masq '1'
        option lan_dns '114.114.114.114'
        option dns_tables '1'
	
