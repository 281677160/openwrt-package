
config global
	option enabled '0'
	option socks_enabled '0'
	option tcp_node 'nil'
	option udp_node 'nil'
	option dns_mode 'pdnsd'
	option up_china_dns 'default'
	option dns_forward '8.8.8.8'
	option tcp_proxy_mode 'chnroute'
	option udp_proxy_mode 'chnroute'
